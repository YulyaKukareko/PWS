@model List<Lab703.Models.VideoModel>
<div class="row">
    <div class="col-2">Number</div>
    <div class="col-5">Original Name</div>
    <div class="col-5">Link</div>
</div>
@for (var i = 0; i < Model.Count; i++)
{
    var obj = Model[i];
    <div class="row">
        <div class="col-2">@i</div>
        <div class="col-5">@obj.OriginalName</div>
        <div class="col-5">@Html.ActionLink("To video", "Video", new { id = obj.Id })</div>
    </div>
}